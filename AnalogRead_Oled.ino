void setup()
{
  Wire.begin();
  SeeedOled.init();  //initialze SEEED OLED display
  SeeedOled.clearDisplay();          
  SeeedOled.drawBitmap((unsigned char*) Dontpanic,1024);
  delay(4000);
  SeeedOled.clearDisplay(); 
  //clear the screen and set start position to top left corner
  SeeedOled.drawBitmap((unsigned char*) Starman,1024);   // 1024 = 128 Pixels * 64 Pixels / 8
  SeeedOled.setNormalDisplay();      //Set display to no rmal mode (i.e non-inverse mode)
  SeeedOled.setPageMode();      
  SeeedOled.setNormalDisplay();
  }

void loop()
{

  // read the input on analog pin 0:
  int sensorValue = analogRead(A0);
  // print out the value you read:
    SeeedOled.setPageMode();           //Set addressing mode to Page Mode
  SeeedOled.setTextXY(2.6,12);          //Set the cursor to Xth Page, Yth Column  
  SeeedOled.putNumber(sensorValue); //Print the String
  delay(100);        // delay in between reads for stability
}

